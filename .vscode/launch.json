{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug application (header)",
            "type": "debugpy",
            "request": "launch",
            "module": "syscall_extract",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "--format=header"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        }
    ]
}